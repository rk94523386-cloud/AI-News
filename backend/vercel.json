{
  "version": 2,
  "buildCommand": "npm install",
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": "**/*.ts"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.ts"
    }
  ]
}
